table.table.fullwidth data-controller="checkbox-selectable"
  thead.bg-transparent
    = render partial: "shared/app_results/sentinelone_header", locals: { show_checkbox: true, show_details: true, show_customer: true, display_admin: current_user.admin? }
  tbody
    = render partial: "shared/app_results/sentinelone_row", collection: similar_detections, as: :app_result, locals: { show_checkbox: true, show_details: true, show_customer: true, show_customer_link: true, query_params: query_params, display_admin: current_user.admin? }, cache: proc { |ar| ["v1", ar, current_user&.timezone] }
