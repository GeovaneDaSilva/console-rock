var tab = document.getElementById('processname-hunt-tab');

<%- if @on_demand_processname.valid? && @hunt.persisted? %>
  // Visit hunt
  Turbolinks.visit('<%= hunt_path(@hunt) %>')
<%- else %>
  // Replace form with one that has errors
  tab.innerHTML = '<%= j render partial: "accounts/hunts/processname", locals: { on_demand_processname: @on_demand_processname } %>'
<%- end %>
