var tab = document.getElementById('filehash-hunt-tab');

<%- if @on_demand_filehash.valid? && @hunt.persisted? %>
  // Visit hunt
  Turbolinks.visit('<%= hunt_path(@hunt) %>')
<%- else %>
  // Replace form with one that has errors
  tab.innerHTML = '<%= j render partial: "accounts/hunts/filehash", locals: { on_demand_filehash: @on_demand_filehash } %>'
<%- end %>
